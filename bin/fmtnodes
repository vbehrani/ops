#!/usr/bin/awk -f

BEGIN {
  FS = ","
  count = 0
}

{
  if (count++ > 0) {
    print ""
  }

  if (!$1) {
    $1 = "<none>"
  }

  if (!$2) {
    $2 = "<none>"
  }

  if (!$3) {
    $3 = "<none>"
  }

  if (!$4) {
    $4 = "<none>"
  }

  if (!$5) {
    $5 = "<none>"
  }

  print "ID\t\t" substr($1, 6, 16)
  print "Name\t\t" $2
  print "Description\t" $3
  print "Location\t" $4
  print "Port\t\t" $5
}
